                CASE WHEN gasto_promedio_trim_anterior IS NOT NULL AND gasto_promedio_trim_actual!=0 
                        THEN gasto_promedio_trim_actual/gasto_promedio_trim_anterior - 1 ELSE NULL END AS var_gasto_trim_actual_anterior,
